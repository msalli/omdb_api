<!doctype html>
<html>
  <head>
    <title>Movie Search Results</title>
  </head>
  <body>
   <h2>Movies Related to Your Search: </h2>
   <ul>
    <% movieList.forEach(function (movie, index) { %>
      <li> <a href="details/<%= movie.imdbID %>"> <%= movie.Title %> </a> </li>
    <% }) %>
   </ul>

  </body>
</html>
